.grid-items(infinite-scroll="$ctrl.updateVideoList()", infinite-scroll-disabled="$ctrl.loaderBusy")
  a.grid-item(ng-repeat="video in $ctrl.videoList", ui-sref="play({ sessionId: $ctrl.sessionId, videoId: video._id})")
    h1 {{ video.name }}
    //videogular
    //  vg-media(vg-src="$ctrl.getUrl(video.url)")
    rating(ratings="video.ratings")
    span(class="player icon ion-arrow-right-b")

